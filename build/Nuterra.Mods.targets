<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Force mods to compile against 3.5. This is the runtime unity supports -->
  <PropertyGroup>
    <TargetFrameworkVersion>v3.5</TargetFrameworkVersion>
  </PropertyGroup>
  <!-- After building the dll, create mono debug symbols -->
  <Target Name="Nuterra_GenerateMonoSymbols" DependsOnTargets="CopyFilesToOutputDirectory">
    <Message Text="MonoSymbols source ==> $(ImmediateAssembly)" />
    <Exec Command="&quot;$(Nuterra_Packages)\Mono.Unofficial.pdb2mdb.4.2.3.4\tools\pdb2mdb.exe&quot; &quot;$(ImmediateAssembly)&quot;"/>
    <ItemGroup>
      <FileWrites Include="$(ImmediateAssembly).mdb" />
      <OutputFiles Include="$(ImmediateAssembly).mdb" />
    </ItemGroup>
    <!-- Todo: Figure out a way to copy the symbol file to referencing projects -->
  </Target>
  <Target Name="Nuterra_GenerateMonoSymbols" DependsOnTargets="CopyFilesToOutputDirectory" AfterTargets="Nuterra_GenerateMonoSymbols">
    <Message Text="MonoSymbols source ==> $(ImmediateAssembly)" />
    <Exec Command="&quot;$(Nuterra_Packages)\Mono.Unofficial.pdb2mdb.4.2.3.4\tools\pdb2mdb.exe&quot; &quot;$(ImmediateAssembly)&quot;"/>
    <ItemGroup>
      <FileWrites Include="$(ImmediateAssembly).mdb" />
      <OutputFiles Include="$(ImmediateAssembly).mdb" />
    </ItemGroup>
    <!-- Todo: Figure out a way to copy the symbol file to referencing projects -->
  </Target>
</Project>